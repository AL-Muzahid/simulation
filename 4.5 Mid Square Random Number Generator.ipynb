   "source": [
    "n = 10 # no of random number generates\n",
    "seed = 4321 # initial_value\n",
    "var = int(len(str(seed))/2)\n",
    "rn = [] # random number lists\n",
    "\n",
    "for i in range(n):\n",
    "    x = seed ** 2\n",
    "    st = str(x)\n",
    "    dig = int((len(st)) / 2)\n",
    "    r = st[dig - var: dig + var]\n",
    "    seed = int(r)\n",
    "    rn.append(seed)\n",
    "rn"
   ]
  }
